<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <servlet>
        <servlet-name>ServletLaudo</servlet-name>
        <servlet-class>Servlets.ServletLaudo</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletSetor</servlet-name>
        <servlet-class>Servlets.ServletSetor</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletEquipamento</servlet-name>
        <servlet-class>Servlets.ServletEquipamento</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletPreliminar</servlet-name>
        <servlet-class>Servlets.ServletPreliminar</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletUsuario</servlet-name>
        <servlet-class>Servlets.ServletUsuario</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletFornecedor</servlet-name>
        <servlet-class>Servlets.ServletFornecedor</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletMateriais</servlet-name>
        <servlet-class>Servlets.ServletMateriais</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>Servlets.Login</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ServletLaudo</servlet-name>
        <url-pattern>/ServletLaudo</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletSetor</servlet-name>
        <url-pattern>/ServletSetor</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletEquipamento</servlet-name>
        <url-pattern>/ServletEquipamento</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletPreliminar</servlet-name>
        <url-pattern>/ServletPreliminar</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletUsuario</servlet-name>
        <url-pattern>/ServletUsuario</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletFornecedor</servlet-name>
        <url-pattern>/ServletFornecedor</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletMateriais</servlet-name>
        <url-pattern>/ServletMateriais</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/Login</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>20</session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>
