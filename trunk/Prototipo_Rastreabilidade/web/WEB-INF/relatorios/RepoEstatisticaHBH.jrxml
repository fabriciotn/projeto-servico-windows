	<queryString>
		<![CDATA[SELECT
  Concat(Concat(to_char(to_date(substr(a.amostra,7,3),'J'), 'dd/mm'),'/'),substr(a.amostra,3,4)) as DATA,
  a.NUMDOADOR,
  a.AMOSTRA,
  a.HOSPITAL,
  h.HOSPITAL as NOME_HOSPITAL,
  t.TIPO as TIPO_DOACAO,
  CASE WHEN a.RESDOACAO = '999' THEN 'APTO' ELSE 'INAPTO' END as RESULTADO
FROM d02_consulta_hbh a, t05_hmae h, t03_hmae t
  WHERE a.DOATIPO = '5'
  AND a.HOSPITAL = '074'
  AND SUBSTR(a.amostra, 3,4) = '2013'
  AND SUBSTR(a.amostra, 7,3) >= '001'
  AND SUBSTR(a.amostra, 7,3) <= '365'
  AND a.HOSPITAL = h.codhosp
  AND a.TIPODOACAO = t.CODTIPO]]>
	</queryString>
